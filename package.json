{
  "name": "dmoj-user-profile",
  "version": "1.0.0",
  "description": "react + express app",
  "main": "server.js",
  "scripts": {
    "start": "concurrently \"npm run server\" \"npm run client\"",
    "server": "nodemon server.js",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "kill": "killall node",
    "heroku-postbuild": "cd client && npm install --only=dev && npm install && npm run build",
    "build": "cd client && npm install && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/stevenbai0724/dmoj-user-profile.git"
  },
  "author": "Steven Bai",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/stevenbai0724/dmoj-user-profile/issues"
  },
  "homepage": "https://github.com/stevenbai0724/dmoj-user-profile#readme",
  "dependencies": {
    "@material-ui/core": "^4.12.1",
    "axios": "^0.21.4",
    "concurrently": "^6.2.1",
    "express": "^4.17.1",
    "node": "^19.3.0",
    "nodemon": "^2.0.13",
    "path": "^0.12.7",
    "react-svg-radar-chart": "^1.3.0"
  },
  "devDependencies": {
    "dotenv": "^14.3.0",
    "react-icons": "^4.3.1"
  },
  "engines": {
    "node": "14.19.3",
    "npm": "6.14.17"
  }
}
